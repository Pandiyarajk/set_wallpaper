[Paths]
# Absolute path to the folder containing your wallpaper images
# If not specified, uses the script's directory
# Example: C:\Users\YourName\Pictures\Wallpapers
image_folder = C:\Users\username\Pictures\desktop_images

[Timing]
# Interval in MINUTES for wallpaper rotation
# Default: 30 minute
interval_minutes = 30

[ImageFormats]
# Supported image file extensions (comma-separated, no spaces)
# Default: jpg,jpeg,png
extensions = jpg,jpeg,png

